<?xml version="1.0" encoding="utf-8"?>
<form>

	<fieldset name="details" label="COM_TAGEBUCH_TAGEBUCH_TAB_DETAILS">

		<field
			name="id"
			type="number"
			label="JGLOBAL_FIELD_ID_LABEL"
			default="0"
			class="readonly"
			readonly="true"
		/>

		<field
			name="datum"
			type="calendar"
			label="COM_TAGEBUCH_TAGEBUCH_DATE_LABEL_DATE"
			default="0"
		/>

		<field
			name="text_fs"
			type="checkbox"
			label="COM_TAGEBUCH_TAGEBUCH_LABEL_TEXT_FS"
			rows="5"
			cols="40"
			required="true"
		 />

		<field
				name="text_ss"
				type="textarea"
				label="COM_TAGEBUCH_TAGEBUCH_LABEL_TEXT_SS"
				rows="5"
				cols="40"
				required="true"
		/>

		<field
				name="text_z1"
				type="textarea"
				label="COM_TAGEBUCH_TAGEBUCH_LABEL_TEXT_Z1"
				rows="5"
				cols="40"
				required="true"
		/>

		<field
				name="text_z2"
				type="textarea"
				label="COM_TAGEBUCH_TAGEBUCH_LABEL_TEXT_Z2"
				rows="5"
				cols="40"
				required="true"
		/>

		<field
			name="published"
			type="list"
			label="JSTATUS"
			default="1"
			id="published"
			class="custom-select-color-state"
			size="1"
			>
			<option value="1">JPUBLISHED</option>
			<option value="0">JUNPUBLISHED</option>
			<option value="2">JARCHIVED</option>
			<option value="-2">JTRASHED</option>
		</field>

	</fieldset>


</form>
